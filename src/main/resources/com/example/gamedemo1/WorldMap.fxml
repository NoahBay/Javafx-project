<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gamedemo1.GameSessionController">
   <children>
      <Label alignment="TOP_CENTER" prefHeight="52.0" prefWidth="600.0" text="Vælg et udsat land for at få et spørgsmål. OBS: det ville være klogt at læse om informationen inden du går igang med spørgsmålene" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="USAButton" layoutX="70.0" layoutY="132.0" mnemonicParsing="false" onAction="#USA" text="USA" />
      <Button fx:id="BrasilienButton" layoutX="91.0" layoutY="286.0" mnemonicParsing="false" text="Brasilien" />
      <Button fx:id="PortugalButton" layoutX="203.0" layoutY="200.0" mnemonicParsing="false" text="Portugal" />
      <Button fx:id="AfricaButton" layoutX="229.0" layoutY="334.0" mnemonicParsing="false" text="Syd Afrika" />
      <Button fx:id="GreeceButton" layoutX="266.0" layoutY="234.0" mnemonicParsing="false" text="Grækenland" />
      <Button fx:id="IndonesiaButton" layoutX="417.0" layoutY="200.0" mnemonicParsing="false" text="Indonesien" />
      <Button fx:id="AustraliaButton" layoutX="417.0" layoutY="321.0" mnemonicParsing="false" text="Australien" />
